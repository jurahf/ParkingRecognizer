{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "Logs\\Log.txt",
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "ParkingTelegramBot"
    }
  },
  "TelegramSettings": {
    "Token": ""
  },
  "RtspProvider": {
    "FfmpegPath": "CamerasWork/{0}/ffmpeg.exe",
    "CameraImageFormat": "Bmp",
    "OutputImagePath": "../../../../../CapturedPhoto",
    "SaveIntervalSeconds": 10
  }
}